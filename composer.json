{
	"name": "ps14/kist-pdf",
	"type": "typo3-cms-extension",
	"description": "Prints the current url as a PDF",
	"keywords": [
		"TYPO3",
		"extension"
	],
	"homepage": "http://ps14.de/",
	"require": {
		"cloudconvert/cloudconvert-php": "^3.4",
		"symfony/http-client": "^7.1",
		"nyholm/psr7": "^1.8"
	},
	"suggest": {
	},
	"autoload": {
		"psr-4": {
			"Ps14\\KistPdf\\": "Classes"
		}
	},
	"config": {
		"vendor-dir": "Vendor",
		"allow-plugins": {
			"php-http/discovery": true,
			"typo3/cms-composer-installers": true,
			"typo3/class-alias-loader": true
		}
	},
	"license": "closed",
	"authors": [
		{
			"name": "Christian Pschorr",
			"email": "pschorr.christian@gmail.com"
		}
	],
	"extra": {
		"typo3/cms": {
			"extension-key": "ps14_kist_pdf"
		}
	}
}
